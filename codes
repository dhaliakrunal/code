package com.example.jsonreader.service;

import com.example.jsonreader.config.ApiProperties;
import com.example.jsonreader.model.*;
import com.fasterxml.jackson.databind.DeserializationFeature;
import com.fasterxml.jackson.databind.JsonNode;
import com.fasterxml.jackson.databind.ObjectMapper;
import lombok.RequiredArgsConstructor;
import org.springframework.http.*;
import org.springframework.stereotype.Service;
import org.springframework.web.client.RestTemplate;

import java.util.ArrayList;
import java.util.List;

@Service
@RequiredArgsConstructor
public class JsonReaderService {

    private final ApiProperties apiProperties;
    private final RestTemplate restTemplate;

    public List<MergedOutput> mergeAndDisplayData() {
        List<MergedOutput> mergedResults = new ArrayList<>();

        try {
            ObjectMapper mapper = new ObjectMapper();
            mapper.configure(DeserializationFeature.FAIL_ON_UNKNOWN_PROPERTIES, false);

            String accessToken = fetchAccessTokenFromHeader();

            HttpHeaders headers = new HttpHeaders();
            headers.setBearerAuth(accessToken);
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));
            headers.set("Accept", "*/*;version=gamma");

            HttpEntity<Void> request = new HttpEntity<>(headers);

            // Fetch entities API data
            ResponseEntity<String> entityResponse = restTemplate.exchange(
                    apiProperties.getEntitiesUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );
            List<Wrapper> wrappers = mapper.readValue(entityResponse.getBody(), EntityResponse.class).getResults();

            // Fetch kyc API data
            ResponseEntity<String> kycResponse = restTemplate.exchange(
                    apiProperties.getKycUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );
            List<KycDetails> kycDetailsList = mapper.readValue(kycResponse.getBody(), KycDetailsWrapper.class).getResults();

            // Matching logic: per kycId
            for (KycDetails kyc : kycDetailsList) {
                if (kyc.getIdentifiers() == null) continue;

                for (Identifiers kycId : kyc.getIdentifiers()) {
                    boolean matchFound = false;

                    for (Wrapper wrapper : wrappers) {
                        if (wrapper.getEntity() == null || wrapper.getEntity().getIdentifiers() == null) continue;

                        for (Identifiers entId : wrapper.getEntity().getIdentifiers()) {
                            if (entId.getPtyId() != null && entId.getPtyId().equals(kycId.getPtyId())) {
                                MergedOutput output = new MergedOutput();

                                // Set only matched ptyId as list
                                output.setIdentifiers(List.of(kycId));
                                output.setBusinessGroup(wrapper.getBusinessGroup());

                                if (kyc.getKycData() != null) {
                                    output.setKycSegment(kyc.getKycData().getKycSegment());
                                    output.setKycSite(kyc.getKycData().getKycSite());
                                    output.setKycRiskIndustry(kyc.getKycData().getRiskIndustry());
                                }

                                mergedResults.add(output);
                                matchFound = true;
                                break;
                            }
                        }

                        if (matchFound) break;
                    }

                    if (!matchFound) {
                        System.out.println("‚ùó No match found for ptyId: " + kycId.getPtyId());
                    }
                }
            }

            return mergedResults;

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error during merge: " + e.getMessage(), e);
        }
    }

    private String fetchAccessTokenFromHeader() {
        try {
            HttpHeaders headers = new HttpHeaders();
            headers.set("Authorization", apiProperties.getTokenAuthHeader());
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));

            HttpEntity<Void> request = new HttpEntity<>(headers);

            ResponseEntity<String> response = restTemplate.exchange(
                    apiProperties.getTokenUrl(),
                    HttpMethod.POST,
                    request,
                    String.class
            );

            if (response.getStatusCode().is2xxSuccessful()) {
                JsonNode json = new ObjectMapper().readTree(response.getBody());
                return json.get("access_token").asText();
            } else {
                throw new RuntimeException("‚ùå Token fetch failed: " + response.getStatusCode());
            }

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error fetching token: " + e.getMessage(), e);
        }
    }
}
package com.example.jsonreader.service;

import com.example.jsonreader.config.ApiProperties;
import com.example.jsonreader.model.*;
import com.fasterxml.jackson.databind.DeserializationFeature;
import com.fasterxml.jackson.databind.JsonNode;
import com.fasterxml.jackson.databind.ObjectMapper;
import lombok.RequiredArgsConstructor;
import org.springframework.http.*;
import org.springframework.stereotype.Service;
import org.springframework.web.client.RestTemplate;

import java.util.ArrayList;
import java.util.List;

@Service
@RequiredArgsConstructor
public class JsonReaderService {

    private final ApiProperties apiProperties;
    private final RestTemplate restTemplate;

    public List<MergedOutput> mergeAndDisplayData() {
        List<MergedOutput> mergedResults = new ArrayList<>();
        List<Integer> unmatchedPtyIds = new ArrayList<>();

        try {
            ObjectMapper mapper = new ObjectMapper();
            mapper.configure(DeserializationFeature.FAIL_ON_UNKNOWN_PROPERTIES, false);

            // ‚úÖ Step 1: Fetch Access Token
            String accessToken = fetchAccessTokenFromHeader();

            // ‚úÖ Step 2: Prepare headers
            HttpHeaders headers = new HttpHeaders();
            headers.setBearerAuth(accessToken);
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));
            headers.set("Accept", "*/*;version=gamma");

            HttpEntity<Void> request = new HttpEntity<>(headers);

            // ‚úÖ Step 3: Fetch Entity API
            ResponseEntity<String> entityResponse = restTemplate.exchange(
                    apiProperties.getEntitiesUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );

            if (!entityResponse.getStatusCode().is2xxSuccessful() || entityResponse.getBody() == null) {
                throw new RuntimeException("‚ùå Failed to fetch entity data. Status: " + entityResponse.getStatusCode());
            }

            List<Wrapper> wrappers = mapper.readValue(entityResponse.getBody(), EntityResponse.class).getResults();

            // ‚úÖ Step 4: Fetch KYC API
            ResponseEntity<String> kycResponse = restTemplate.exchange(
                    apiProperties.getKycUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );

            if (!kycResponse.getStatusCode().is2xxSuccessful() || kycResponse.getBody() == null) {
                throw new RuntimeException("‚ùå Failed to fetch KYC data. Status: " + kycResponse.getStatusCode());
            }

            List<KycDetails> kycDetailsList = mapper.readValue(kycResponse.getBody(), KycDetailsWrapper.class).getResults();

            // ‚úÖ Step 5: Merge using ptyId
            for (Wrapper wrapper : wrappers) {
                if (wrapper.getEntity() == null || wrapper.getEntity().getIdentifiers() == null) continue;

                for (Identifiers entityId : wrapper.getEntity().getIdentifiers()) {
                    boolean matchFound = false;

                    for (KycDetails kyc : kycDetailsList) {
                        if (kyc.getIdentifiers() == null) continue;

                        boolean match = kyc.getIdentifiers().stream()
                                .anyMatch(kycId -> kycId.getPtyId() != null && kycId.getPtyId().equals(entityId.getPtyId()));

                        if (match) {
                            MergedOutput output = new MergedOutput();
                            output.setIdentifiers(wrapper.getEntity().getIdentifiers());
                            output.setBusinessGroup(wrapper.getBusinessGroup());

                            if (kyc.getKycData() != null) {
                                output.setKycSegment(kyc.getKycData().getKycSegment());
                                output.setKycSite(kyc.getKycData().getKycSite());
                                output.setKycRiskIndustry(kyc.getKycData().getRiskIndustry());
                            }

                            mergedResults.add(output);
                            matchFound = true;
                        }
                    }

                    // ‚úÖ If no match found for this ptyId
                    if (!matchFound) {
                        unmatchedPtyIds.add(entityId.getPtyId());
                    }
                }
            }

            // ‚úÖ Log all unmatched ptyIds
            System.out.println("üîç Total unmatched ptyIds: " + unmatchedPtyIds.size());
            System.out.println("Unmatched ptyIds: " + unmatchedPtyIds);

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error during merging: " + e.getMessage(), e);
        }

        return mergedResults;
    }

    // üîê Token fetch logic
    private String fetchAccessTokenFromHeader() {
        try {
            HttpHeaders headers = new HttpHeaders();
            headers.set("Authorization", apiProperties.getTokenAuthHeader());
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));

            HttpEntity<Void> request = new HttpEntity<>(headers);

            ResponseEntity<String> response = restTemplate.exchange(
                    apiProperties.getTokenUrl(),
                    HttpMethod.POST,
                    request,
                    String.class
            );

            if (response.getStatusCode().is2xxSuccessful()) {
                JsonNode json = new ObjectMapper().readTree(response.getBody());
                return json.get("access_token").asText();
            } else {
                throw new RuntimeException("‚ùå Token fetch failed: " + response.getStatusCode());
            }

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error fetching token: " + e.getMessage(), e);
        }
    }
}
package com.example.jsonreader.service;

import com.example.jsonreader.config.ApiProperties;
import com.example.jsonreader.model.*;
import com.fasterxml.jackson.databind.DeserializationFeature;
import com.fasterxml.jackson.databind.JsonNode;
import com.fasterxml.jackson.databind.ObjectMapper;
import lombok.RequiredArgsConstructor;
import org.springframework.http.*;
import org.springframework.stereotype.Service;
import org.springframework.web.client.RestTemplate;

import java.util.ArrayList;
import java.util.List;

@Service
@RequiredArgsConstructor
public class JsonReaderService {

    private final ApiProperties apiProperties;
    private final RestTemplate restTemplate;

    public List<MergedOutput> mergeAndDisplayData() {
        List<MergedOutput> mergedResults = new ArrayList<>();

        try {
            ObjectMapper mapper = new ObjectMapper();
            mapper.configure(DeserializationFeature.FAIL_ON_UNKNOWN_PROPERTIES, false);

            // ‚úÖ Step 1: Get access token from API using only header
            String accessToken = fetchAccessTokenFromHeader();

            // ‚úÖ Step 2: Prepare headers with that token
            HttpHeaders headers = new HttpHeaders();
            headers.setBearerAuth(accessToken);
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));
            headers.set("Accept", "*/*;version=gamma");

            HttpEntity<Void> request = new HttpEntity<>(headers);

            // ‚úÖ Step 3: Fetch entity data
            ResponseEntity<String> entityResponse = restTemplate.exchange(
                    apiProperties.getEntitiesUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );
            List<Wrapper> wrappers = mapper.readValue(entityResponse.getBody(), EntityResponse.class).getResults();

            // ‚úÖ Step 4: Fetch KYC data
            ResponseEntity<String> kycResponse = restTemplate.exchange(
                    apiProperties.getKycUrl(),
                    HttpMethod.GET,
                    request,
                    String.class
            );
            List<KycDetails> kycDetailsList = mapper.readValue(kycResponse.getBody(), KycDetailsWrapper.class).getResults();

            // ‚úÖ Step 5: Merge on ptyId
            for (Wrapper wrapper : wrappers) {
                if (wrapper.getEntity() == null || wrapper.getEntity().getIdentifiers() == null) continue;

                for (Identifiers id : wrapper.getEntity().getIdentifiers()) {
                    for (KycDetails kyc : kycDetailsList) {
                        if (kyc.getIdentifiers() == null) continue;

                        boolean match = kyc.getIdentifiers().stream()
                                .anyMatch(kid -> kid.getPtyId() != null && kid.getPtyId().equals(id.getPtyId()));

                        if (match) {
                            MergedOutput output = new MergedOutput();
                            output.setIdentifiers(wrapper.getEntity().getIdentifiers());
                            output.setBusinessGroup(wrapper.getBusinessGroup());

                            if (kyc.getKycData() != null) {
                                output.setKycSegment(kyc.getKycData().getKycSegment());
                                output.setKycSite(kyc.getKycData().getKycSite());
                                output.setKycRiskIndustry(kyc.getKycData().getRiskIndustry());
                            }

                            mergedResults.add(output);
                            break;
                        }
                    }
                }
            }

            return mergedResults;

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error during merge: " + e.getMessage(), e);
        }
    }

    // üîê Fetch token using Authorization header only
    private String fetchAccessTokenFromHeader() {
        try {
            HttpHeaders headers = new HttpHeaders();
            headers.set("Authorization", apiProperties.getTokenAuthHeader()); // e.g. "Basic abc123==" or "Bearer xyz"
            headers.setAccept(List.of(MediaType.APPLICATION_JSON));

            HttpEntity<Void> request = new HttpEntity<>(headers);

            ResponseEntity<String> response = restTemplate.exchange(
                    apiProperties.getTokenUrl(),
                    HttpMethod.POST,
                    request,
                    String.class
            );

            if (response.getStatusCode().is2xxSuccessful()) {
                JsonNode json = new ObjectMapper().readTree(response.getBody());
                return json.get("access_token").asText(); // assumes token is returned as JSON: { "access_token": "..." }
            } else {
                throw new RuntimeException("‚ùå Token fetch failed: " + response.getStatusCode());
            }

        } catch (Exception e) {
            throw new RuntimeException("‚ùå Error fetching token: " + e.getMessage(), e);
        }
    }
}



api.token-url=https://api.example.com/token
api.token-auth-header=Basic abc123==     # or Bearer xyz123
api.entities-url=https://api.example.com/entities
api.kyc-url=https://api.example.com/kyc



